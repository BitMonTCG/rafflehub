  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:generate-pg": "cross-env NODE_ENV=production drizzle-kit generate:pg --config ./drizzle.config.ts",
    "db:migrate-pg": "cross-env NODE_ENV=production npx drizzle-kit up:pg --config ./drizzle.config.ts",
    "db:push-sqlite": "cross-env NODE_ENV=development drizzle-kit push",
    "db:migrate-sqlite": "cross-env NODE_ENV=development drizzle-kit migrate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
